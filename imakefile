OBJS = pilrc.o lex.o util.o bitmap.o main.o font.o plex.o
XOBJS = pilrc.o lex.o util.o bitmap.o xwin.o font.o plex.o

LOCLIBS = $(XPMLIB) $(XLIB)

XLIBS = -L/local/lib -lgtk -lgdk -lglib $(XLIB) -lm


AllTarget (pilrc pilrcui)

NormalProgramTarget(pilrc,$(OBJS),$(DEPLIBS),$(LOCLIBS),$(SYSLIBS))

NormalProgramTarget(pilrcui,$(XOBJS),$(DEPLIBS),$(XLIBS),$(SYSLIBS))
